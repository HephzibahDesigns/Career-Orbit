{
  "expo": {
    "name": "react-native-job-finder-ui",
    "slug": "react-native-job-finder-ui",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/career-orbit.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/orbit3.png",
      "resizeMode": "cover",
      "backgroundColor": "#406ce0"
    },
    "assetBundlePatterns": ["assets/*"],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/orbit3.png",
        "resizeMode": "cover",
        "backgroundColor": "#406ce0"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/facebook_logo.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/orbit3.png",
        "resizeMode": "cover",
        "backgroundColor": "#406ce0"
      },
      "package": "com.hephzibah.reactnativejobfinderui"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "newArchEnabled": true
  }
}
